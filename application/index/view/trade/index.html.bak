    <style>
        .numShow{
            overflow: hidden;
            margin: 10px;
            background: url("/assets/img/trade/tradetop-bg.png") no-repeat;
            background-size: 100%;
            color: #fff;
            border-radius: 5px;
        }
        .numShow img{
            display: inline-block;
            height: 12px;
            margin: 0 3px 0 8px;
            position: relative;
            top:2px
        }
        .numShow .meijin{
            padding-left: 15px;
            font-size: 15px;
            line-height: 45px;
            position: relative;
        }
        .numShow .meijin span{
            position: absolute;
            top: 10px;
            right: 1px;
            height: 23px;
            line-height: 23px;
            background: #155ddc;
            font-size: 12px;
            border-radius: 10px 0 0 10px;
            padding-right: 10px;
        }
        .numShow ul{
            overflow: hidden;
        }
        .numShow ul li{
            float: left;
            width: 49.33%;
            margin: 15px 0;
            text-align: center;
            letter-spacing: 1px;
        }
        .numShow li div{
            height: 30px;
            line-height: 30px;
            letter-spacing: 1px;
            font-size: 18px;
        }
        .numShow li span{
            display: block;
            font-size: 13px;
            line-height: 20px;
            text-align: center;
        }
        .numShow .zichan{
            line-height: 35px;
        }
        .numShow .zichan span{
            float: left;
            width: 50%;
            padding-left: 10px;
            box-sizing: border-box;
            overflow: hidden;
        }
        .searchBox{
            background: #e8eceb;
            padding: 10px;
            position: relative;
            margin-top: 10px;
        }
        .searchBox img{
            position: absolute;
            top:21px;
            left: 20px;
            height: 15px;
        }
        .searchBox input{
            width: 100%;
            background: #FFF;
            border: none;
            border-radius: 5px;
            height: 35px;
            line-height: 35px;
            padding-left: 40px;
            font-size: 14px;
            font-family: "Microsoft YaHei";
        }
        .trade-tab ul{
            overflow: hidden;
        }
        .trade-tab ul li{
            float: left;
            width: 25%;
            border-bottom: 2px solid #e5e5e5;
        }
        .trade-tab ul li.active{
            border-bottom: 2px solid #007aff;
        }
        .trade-tab ul li a{
            height: 50px;
            line-height: 50px;
            display: block;
            font-size: 16px;
            color: #333;
            font-weight: 600;
            text-align: center;
        }
        .trade-tab ul li.active a{
            color: #007aff;
        }
        .showNum{
            overflow: hidden;
        }
        .showNum li{
            float: left;
            width: 33.33%;
            padding-bottom: 5px;
        }
        .showNum li p{
            font-size: 14px;
            color: #777;
            line-height: 25px;
        }
        .showNum li p:nth-of-type(2){
